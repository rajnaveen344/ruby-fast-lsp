{
  "name": "ruby-fast-lsp",
  "displayName": "Ruby Fast LSP",
  "description": "A fast Language Server Protocol implementation for Ruby",
  "version": "0.1.0",
  "publisher": "ruby-fast-lsp-team",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruby-fast-lsp-team/ruby-fast-lsp"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ruby",
        "aliases": [
          "Ruby",
          "ruby"
        ],
        "extensions": [
          ".rb",
          ".rake",
          ".gemspec",
          "Gemfile",
          "Rakefile"
        ]
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  }
}
