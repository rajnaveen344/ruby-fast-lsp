{
  "name": "ruby-fast-lsp",
  "displayName": "Ruby Fast LSP",
  "description": "High-performance Ruby Language Server with fast code navigation, syntax highlighting, and intelligent code insights for Ruby developers",
  "version": "2025.26.0",
  "publisher": "naveenraj",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajnaveen344/ruby-fast-lsp"
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters",
    "Snippets",
    "Language Packs"
  ],
  "keywords": [
    "ruby",
    "lsp",
    "language-server",
    "intellisense",
    "code-navigation",
    "productivity",
    "development"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ruby",
        "aliases": [
          "Ruby",
          "ruby"
        ],
        "extensions": [
          ".rb",
          ".rake",
          ".gemspec",
          "Gemfile",
          "Rakefile"
        ]
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}